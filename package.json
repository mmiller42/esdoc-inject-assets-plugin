{
  "name": "esdoc-inject-assets-plugin",
  "version": "1.0.0",
  "description": "ESDoc plugin for adding custom stylesheets and scripts to the template.",
  "main": "lib/index.js",
  "scripts": {
    "precommit": "lint-staged",
    "prettier": "prettier --write '{src/**/*.{js,json},*.js,.*rc,.*.json}'"
  },
  "lint-staged": {
    "{src/**/*.{js,json},*.js,.*rc,.*.json}": [
      "prettier --write",
      "git add"
    ]
  },
  "engines": {
    "node": ">=4.3.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mmiller42/esdoc-inject-assets-plugin.git"
  },
  "author": "Matt Miller <me@mmiller.me>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mmiller42/esdoc-inject-assets-plugin/issues"
  },
  "homepage": "https://github.com/mmiller42/esdoc-inject-assets-plugin#readme",
  "devDependencies": {
    "esdoc": "^1.0.2",
    "husky": "^0.14.3",
    "lint-staged": "^4.2.1",
    "prettier": "^1.7.0"
  },
  "peerDependencies": {
    "esdoc": "^1.0.2"
  }
}
